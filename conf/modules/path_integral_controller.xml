<!DOCTYPE module SYSTEM "module.dtd">

<module name="computation_time_v2">
  <doc>
    <description>Run a path integral algorithm and measure the running time needed to perform the computations</description>
  </doc>
  <header>
    <file name="computation_time.h"/>
  </header>
  <init fun="pi_init()"/>
  <periodic fun="compute_optimal_controls_periodic()" freq="15"/>
  <makefile target="ap">
    <file name="computation_time.c"/>
    <file name="pi_controller.c"/>
    <flag name="LDFLAGS" value="lgsl"/>
  </makefile>
</module>
